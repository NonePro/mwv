{
  "name": "markwhen",
  "displayName": "Markwhen",
  "description": "Visualize and edit markwhen documents",
  "version": "1.4.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mark-when/vscode.git"
  },
  "icon": "icons/markwhen.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/kochrt"
  },
  "publisher": "Markwhen",
  "keywords": [
    "timeline",
    "gantt",
    "calendar"
  ],
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "configurationDefaults": {
    "markwhen": {
      "editor.semanticHighlighting.enabled": true
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "markwhen.openPreview",
        "title": "Open preview to the side",
        "category": "Markwhen",
        "icon": "$(open-preview)"
      },
      {
        "command": "markwhen.viewInTimeline",
        "title": "View event in timeline",
        "category": "Markwhen",
        "icon": "$(arrow-right)"
      },
      {
        "command": "markwhen.timelineView",
        "title": "Timeline view",
        "category": "Markwhen",
        "icon": {
          "light": "icons/timeline.svg",
          "dark": "icons/timeline_dark.svg"
        }
      },
      {
        "command": "markwhen.calendarView",
        "title": "Calendar view",
        "category": "Markwhen",
        "icon": {
          "light": "icons/calendar.svg",
          "dark": "icons/calendar_dark.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "markwhen.openPreview",
          "when": "editorLangId == markwhen"
        }
      ],
      "editor/title": [
        {
          "command": "markwhen.openPreview",
          "when": "editorLangId == markwhen",
          "group": "navigation"
        },
        {
          "command": "markwhen.timelineView",
          "when": "activeCustomEditorId == markwhen.timeline",
          "group": "navigation"
        },
        {
          "command": "markwhen.calendarView",
          "when": "activeCustomEditorId == markwhen.timeline",
          "group": "navigation"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "markwhen.timeline",
        "displayName": "Markwhen",
        "selector": [
          {
            "filenamePattern": "*.mw"
          }
        ],
        "priority": "option"
      }
    ],
    "semanticTokenTypes": [],
    "languages": [
      {
        "id": "markwhen",
        "aliases": [
          "Markwhen",
          "mw"
        ],
        "extensions": [
          ".mw"
        ],
        "icon": {
          "dark": "./icons/icon.svg",
          "light": "./icons/icon.svg"
        },
        "filenamePatterns": [
          "*.mw"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./ && node scripts/copyAssets.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "link:parser": "yalc add @markwhen/parser && yalc link @markwhen/parser && npm install",
    "unlink:parser": "yalc remove @markwhen/parser && npm install",
    "deploy": "vsce publish",
    "package": "vsce package",
    "dev": "vite",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/luxon": "^3.3.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.43",
    "@types/vscode": "^1.81.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "@vscode/vsce": "^2.21.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  },
  "dependencies": {
    "@markwhen/calendar": "^1.3.5",
    "@markwhen/parser": "^0.11.4",
    "@markwhen/timeline": "^1.4.3",
    "@markwhen/view-client": "^1.5.2",
    "node-html-parser": "^6.1.6",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^8.0.2",
    "luxon": "^3.3.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.11"
  },
  "bugs": {
    "url": "https://github.com/mark-when/vscode/issues"
  },
  "homepage": "https://github.com/mark-when/vscode#readme",
  "directories": {
    "doc": "docs"
  },
  "author": "",
  "license": "ISC"
}
